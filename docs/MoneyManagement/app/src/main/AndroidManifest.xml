<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arnav.moneymanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".CreatepasswordActivity" />
        <activity android:name=".EnterPasswordActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".IncomeActivity" />
        <activity android:name=".SettingsActivity"/>
        <activity android:name=".IncomefilterActivity"/>
        <activity android:name=".ExpensefilterActivity"/>
        <activity android:name=".SplashActivity"/>
        <activity android:name=".AuditActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>